#: ../src/gnome-screensaver.c:76 ../src/gnome-screensaver-command.c:58
#: ../src/gnome-screensaver-dialog.c:59
msgid "Version of this application"
msgstr "Versie van deze toepassing"

#: ../src/gnome-screensaver.c:77
msgid "Don't become a daemon"
msgstr "Niet als daemon uitvoeren"

#: ../src/gnome-screensaver.c:78
msgid "Enable debugging code"
msgstr "Debugging-code gebruiken"

#: ../src/gnome-screensaver-command.c:46
msgid "Causes the screensaver to exit gracefully"
msgstr "Zorgt dat de schermbeveiliging netjes afsluit"

#: ../src/gnome-screensaver-command.c:48
msgid "Query the state of the screensaver"
msgstr "De toestand van de schermbeveiliging opvragen"

#: ../src/gnome-screensaver-command.c:50
msgid "Query the length of time the screensaver has been active"
msgstr "Opvragen hoe lang de schermbeveiliging actief is geweest"

#: ../src/gnome-screensaver-command.c:52
msgid "Tells the running screensaver process to lock the screen immediately"
msgstr ""
"Geeft de actieve schermbeveiliging opdracht om het scherm onmiddellijk te "
"vergrendelen"

#: ../src/gnome-screensaver-command.c:54
msgid "Turn the screensaver on (blank the screen)"
msgstr "De schermbeveiliging activeren (het scherm zwart maken)"

#: ../src/gnome-screensaver-command.c:56
msgid "If the screensaver is active then deactivate it (un-blank the screen)"
msgstr ""
"Indien de schermbeveiliging actief is, deze deactiveren (het scherm "
"zichtbaar maken)"

#: ../src/gnome-screensaver-command.c:224
msgid "The screensaver is active\n"
msgstr "De schermbeveiliging is actief\n"

#: ../src/gnome-screensaver-command.c:226
msgid "The screensaver is inactive\n"
msgstr "De schermbeveiliging is niet actief\n"

#: ../src/gnome-screensaver-command.c:256
#, c-format
msgid "The screensaver has been active for %d second.\n"
msgid_plural "The screensaver has been active for %d seconds.\n"
msgstr[0] "De schermbeveiliging was voor %d seconden actief.\n"
msgstr[1] "De schermbeveiliging was voor %d seconden actief.\n"

#: ../src/gnome-screensaver-command.c:258
msgid "The screensaver is not currently active.\n"
msgstr "De schermbeveiliging is momenteel niet actief.\n"

#: ../src/gnome-screensaver-dialog.c:57
msgid "Show debugging output"
msgstr "Debug uitvoer tonen"

#: ../src/gnome-screensaver-dialog.c:61
msgid "Show the logout button"
msgstr "Een knop voor afmelden tonen"

#: ../src/gnome-screensaver-dialog.c:63
msgid "Command to invoke from the logout button"
msgstr "Opdracht die wordt uitgevoerd met de knop voor afmelden"

#: ../src/gnome-screensaver-dialog.c:65
msgid "Show the switch user button"
msgstr "Een knop tonen met 'wissel gebruiker'"

#: ../src/gnome-screensaver-dialog.c:67
msgid "Message to show in the dialog"
msgstr "Te tonen bericht in het dialoogvenster"

#. Translators: This is the example input for the --status-message command
#. line option.
#. Translators: This is the example input for the --away-message command line
#. option.
#: ../src/gnome-screensaver-dialog.c:69 ../src/gnome-screensaver-dialog.c:73
msgid "MESSAGE"
msgstr "BERICHT"

#: ../src/gnome-screensaver-dialog.c:71
msgid "Not used"
msgstr "Niet gebruikt"

#. login: is whacked always translate to Username:
#: ../src/gnome-screensaver-dialog.c:185 ../src/gnome-screensaver-dialog.c:186
#: ../src/gnome-screensaver-dialog.c:187 ../src/gs-auth-pam.c:691
msgid "Username:"
msgstr "Gebruikersnaam:"

#: ../src/gnome-screensaver-dialog.c:188 ../src/gnome-screensaver-dialog.c:189
#: ../src/gs-auth-pam.c:156
msgid "Password:"
msgstr "Wachtwoord:"

#: ../src/gnome-screensaver-dialog.c:190
msgid "You are required to change your password immediately (password aged)"
msgstr "U dient uw wachtwoord direct te veranderen (wachtwoord verlopen)"

#: ../src/gnome-screensaver-dialog.c:191
msgid "You are required to change your password immediately (root enforced)"
msgstr ""
"U dient uw wachtwoord direct te veranderen (op last van de systeembeheerder)"

#: ../src/gnome-screensaver-dialog.c:192
msgid "Your account has expired; please contact your system administrator"
msgstr "Uw account is verlopen; neem contact op met de systeembeheerder"

#: ../src/gnome-screensaver-dialog.c:193
msgid "No password supplied"
msgstr "Geen wachtwoord opgegeven"

#: ../src/gnome-screensaver-dialog.c:194
msgid "Password unchanged"
msgstr "Wachtwoord ongewijzigd"

#: ../src/gnome-screensaver-dialog.c:195
msgid "Cannot get username"
msgstr "Kan gebruikersnaam niet opvragen"

#: ../src/gnome-screensaver-dialog.c:196
msgid "Retype new Unix password:"
msgstr "Nieuw Unix-wachtwoord (opnieuw):"

#: ../src/gnome-screensaver-dialog.c:197
msgid "Enter new Unix password:"
msgstr "Nieuw Unix-wachtwoord:"

#: ../src/gnome-screensaver-dialog.c:198
msgid "(current) Unix password:"
msgstr "Huidig Unix-wachtwoord:"

#: ../src/gnome-screensaver-dialog.c:199
msgid "Error while changing NIS password."
msgstr "Fout bij veranderen NIS-wachtwoord."

#: ../src/gnome-screensaver-dialog.c:200
msgid "You must choose a longer password"
msgstr "U dient een langer wachtwoord te gebruiken"

#: ../src/gnome-screensaver-dialog.c:201
msgid "Password has been already used. Choose another."
msgstr "Dit wachtwoord is al eens gebruikt. Kies een nieuw wachtwoord."

#: ../src/gnome-screensaver-dialog.c:202
msgid "You must wait longer to change your password"
msgstr "U dient langer te wachten om uw wachtwoord te veranderen"

#: ../src/gnome-screensaver-dialog.c:203
msgid "Sorry, passwords do not match"
msgstr "Helaas, de wachtwoorden komen niet overeen"

#: ../src/gnome-screensaver-dialog.c:269
msgid "Checking…"
msgstr "Controle…"

#: ../src/gnome-screensaver-dialog.c:311 ../src/gs-auth-pam.c:438
msgid "Authentication failed."
msgstr "Controle mislukt."

#: ../src/gs-listener-dbus.c:1302
msgid "failed to register with the message bus"
msgstr "kon niet registreren bij de message bus"

#: ../src/gs-listener-dbus.c:1312
msgid "not connected to the message bus"
msgstr "kon verbonden met de message bus"

#: ../src/gs-listener-dbus.c:1339
msgid "screensaver already running in this session"
msgstr "schermbeveiliging al actief in deze sessie"

#: ../src/gs-lock-plug.c:293
msgid "Time has expired."
msgstr "Tijd is verlopen."

#: ../src/gs-lock-plug.c:324
msgid "You have the Caps Lock key on."
msgstr "Caps Lock staat aan."

#: ../src/gs-lock-plug.c:1406
msgid "S_witch User…"
msgstr "_Gebruiker wisselen…"

#: ../src/gs-lock-plug.c:1415
msgid "Log _Out"
msgstr "_Afmelden"

#: ../src/gs-lock-plug.c:1422
msgid "_Unlock"
msgstr "_Ontgrendelen"

#: ../src/gs-lock-plug.c:1593
msgid "_Password:"
msgstr "_Wachtwoord:"

#: ../src/gs-auth-pam.c:380
#, c-format
msgid "Unable to establish service %s: %s\n"
msgstr "Instellen van service ‘%s’ mislukt: %s\n"

#: ../src/gs-auth-pam.c:406
#, c-format
msgid "Can't set PAM_TTY=%s"
msgstr "Instellen ‘PAM_TTY=%s’ mislukt"

#: ../src/gs-auth-pam.c:436
msgid "Incorrect password."
msgstr "Ongeldig wachtwoord."

#: ../src/gs-auth-pam.c:452
msgid "Not permitted to gain access at this time."
msgstr "U heeft geen toegang op dit moment."

#: ../src/gs-auth-pam.c:458
msgid "No longer permitted to access the system."
msgstr "U heeft niet langer toegang tot deze computer."

#: ../data/budgie-screensaver.desktop.in.h:1
msgid "Screensaver"
msgstr "Schermbeveiliging"

#: ../data/budgie-screensaver.desktop.in.h:2
msgid "Launch screensaver and locker program"
msgstr "Schermbeveiliging en -vergrendeling opstarten"
